{
  "name": "coaching",
  "version": "1.0.0",
  "description": "RFNG Coaching Service",
  "main": "./src/index.ts",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "dotenv -e .env.development -- nodemon src/index.ts",
    "start": "node dist/index.js",
    "build": "rm -rf ./dist && dotenv -e .env.production -- npx prisma generate && npx tsc",
    "build:staging": "rm -rf ./dist && dotenv -e .env.staging -- npx prisma generate && npx tsc",
    "build:beta": "rm -rf ./dist && dotenv -e .env.beta -- npx prisma generate && npx tsc",
    "prisma:dev:generate": "dotenv -e .env.development -- npx prisma generate",
    "prisma:dev:pull": "dotenv -e .env.development -- npx prisma db pull"
  },
  "author": "Rapidfunnel",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^6.5.0",
    "dotenv": "^16.4.5",
    "express": "^5.0.1",
    "joi": "^17.13.3",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@types/express": "^5.0.0",
    "@types/node": "^20.16.2",
    "dotenv-cli": "^7.4.2",
    "nodemon": "^3.1.4",
    "prisma": "^6.5.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2"
  }
}
