  "scripts": {
    "start": "vite",
    "build-staging": "dotenv -e .env.staging vite build",
    "build-beta": "dotenv -e .env.beta vite build",
    "build-production": "dotenv -e .env.production vite build",
    "build": "tsc && vite build",
    "test": "test",
    "e2etest": "CYPRESS_BASE_URL=http://localhost:3000 npx cypress open",
    "cypress:open": "npx cypress open",
    "lint": "eslint .",
    "prepare": "cd .. && husky frontend/.husky",
    "tolgeeLogin": "eval $(grep '^TOLGEE_API_KEY' .env) && npx @tolgee/cli login ${TOLGEE_API_KEY}",
    "tolgeePull": "npm run tolgeeLogin && npx @tolgee/cli pull --empty-dir"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.16.0",
    "@testing-library/jest-dom": "^5.16.5",
