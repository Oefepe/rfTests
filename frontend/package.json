    "cypress": "^12.17.4",
    "dotenv-cli": "^7.3.0",
    "eslint": "^9.16.0",
    "eslint-plugin-cypress": "^4.1.0",
    "eslint-plugin-react": "^7.37.2",
    "globals": "^15.13.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "react-world-flags": "^1.6.0",
    "sass": "^1.62.1",
    "typescript": "^4.9.5",
    "typescript-eslint": "^8.17.0"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --max-warnings=0"
    ]
  }
